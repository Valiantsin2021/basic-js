{
  "name": "basic-js",
  "version": "1.0.0",
  "description": "Tasks for RSS",
  "main": "index.js",
  "engines": {
    "node": "20",
    "npm": "9"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha --reporter mocha-multi-reporters --reporter-options configFile=config.json",
    "posttest": "allure generate --clean",
    "format": "prettier --write ./src/**/*.js",
    "lint": "eslint ./src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlreadyBored/basic-js.git"
  },
  "keywords": [
    "RSS",
    "tasks"
  ],
  "author": "AlreadyBored",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlreadyBored/basic-js/issues"
  },
  "homepage": "https://github.com/AlreadyBored/basic-js#readme",
  "devDependencies": {
    "chai": "4.3.10",
    "mocha": "10.2.0",
    "prettier": "3.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-prettier": "^5.0.1",
    "mocha-allure-reporter": "^1.4.0",
    "mocha-junit-reporter": "^2.2.1",
    "mocha-multi-reporters": "^1.5.1",
    "sinon": "14.0.0"
  },
  "dependencies": {
    "allure-commandline": "^2.25.0"
  }
